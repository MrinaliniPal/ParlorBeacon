<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.springboardsolution.parlorbeacon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Select" />
        <activity android:name=".Shopkeeper" />
        <activity android:name=".Customer" />
        <activity android:name=".LoginShop" />
        <activity android:name=".LoginCust" />
        <activity android:name=".SignUpShop" />
        <activity android:name=".Forgot" />
        <activity android:name=".SignUpCust"></activity>
    </application>

</manifest>